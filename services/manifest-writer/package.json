{
  "name": "@ecco/manifest-writer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "start": "node dist/index.js",
    "dev": "sh -lc 'npm run build:watch & node --watch dist/index.js'"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.0",
    "@ecco/platform-libs": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.5.0"
  }
}
